{
  "name": "@pxp-nd/repositories",
  "version": "1.0.7",
  "description": "Core repositories for pxp-nd",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "prepublish": "tsc",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "dev:publish": "yalc publish",
    "prod:publish": "npm publish",
    "start": "npm run build && node dist/index.js",
    "dev": "tsc-watch --target es2017 --outDir ./dist --onSuccess \"node dist/index.js\"",
    "build:js": "babel src --out-dir dist --extensions \".ts\" --source-maps inline",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "pxp-nd"
  ],
  "author": "Kplian",
  "license": "ISC",
  "devDependencies": {
    "rimraf": "^3.0.2",
    "tsc-watch": "^4.2.9"
  },
  "dependencies": {
    "@pxp-nd/core": "file:.yalc/@pxp-nd/core",
    "@pxp-nd/entities": "file:.yalc/@pxp-nd/entities",
    "class-validator": "^0.14.1",
    "moment": "2.29.1",
    "typeorm": "^0.3.20"
  }
}
